              {Array.isArray(customers) && customers.map((customer: any, index: number) => (
                <TableRow key={index}>
                  <TableCell className="font-medium">{customer.firstName} {customer.lastName}</TableCell>
                  <TableCell>{customer.email}</TableCell>
